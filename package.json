{
  "name": "npm-reflect",
  "type": "module",
  "version": "1.1.0",
  "description": "Check npm package dependencies, stats and impact on your package before installing it",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "example": "./bin/index.js install commander",
    "mocha": "mocha --require chai/register-expect.js",
    "c8": "c8 --all npm run mocha",
    "test": "npm run lint && npm run c8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brettz9/npm-reflect.git"
  },
  "keywords": [
    "npm",
    "install",
    "stats",
    "package"
  ],
  "author": "Brett Zamir",
  "contributors": [
    "delfrrr"
  ],
  "license": "(MIT AND Artistic-2.0)",
  "bin": {
    "npm-reflect": "./bin/index.js"
  },
  "bugs": {
    "url": "https://github.com/brettz9/npm-reflect/issues"
  },
  "homepage": "https://github.com/brettz9/npm-reflect#readme",
  "engines": {
    "node": ">=12.20.0"
  },
  "c8": {
    "exclude": [
      ".idea/**",
      ".eslintrc.js",
      ".ncurc.js",
      "test/**"
    ]
  },
  "devDependencies": {
    "@brettz9/eslint-plugin": "^1.0.4",
    "c8": "^7.11.0",
    "chai": "^4.3.6",
    "eslint": "^8.9.0",
    "eslint-config-ash-nazg": "32.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-array-func": "^3.1.7",
    "eslint-plugin-compat": "^4.0.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^37.9.1",
    "eslint-plugin-markdown": "^2.2.1",
    "eslint-plugin-no-unsanitized": "^4.0.1",
    "eslint-plugin-no-use-extend-native": "^0.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-sonarjs": "^0.11.0",
    "eslint-plugin-unicorn": "^40.1.0",
    "mocha": "^9.2.0"
  },
  "dependencies": {
    "cli-table3": "^0.6.1",
    "colors": "^1.4.0",
    "commander": "^9.0.0",
    "filesize": "^8.0.7",
    "inquirer": "^8.2.0",
    "moment": "^2.29.1",
    "node-fetch": "^3.2.0",
    "promise-queue": "^2.2.5",
    "rc": "^1.2.8",
    "semver": "^7.3.5",
    "spdx-correct": "^3.1.1",
    "spdx-satisfies": "^5.0.1"
  }
}
